<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>EHealth</display-name>
  <welcome-file-list>
   
    <welcome-file>login.jsp</welcome-file>
  </welcome-file-list>
  
  
  <servlet>
    <servlet-name>LoginRegister</servlet-name>
    <servlet-class>controller.LoginRegister</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>LoginRegister</servlet-name>
    <url-pattern>/LoginRegister</url-pattern>  
    <!-- This is the URL of the servlet. -->
</servlet-mapping>

  <servlet>
    <servlet-name>GetInformation</servlet-name>
    <servlet-class>controller.GetInformation</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>GetInformation</servlet-name>
    <url-pattern>/GetInformation</url-pattern>  
    <!-- This is the URL of the servlet. -->
</servlet-mapping>


  <servlet>
    <servlet-name>GeneratePdf</servlet-name>
    <servlet-class>controller.GeneratePdf</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>GeneratePdf</servlet-name>
    <url-pattern>/GeneratePdf</url-pattern>  
    <!-- This is the URL of the servlet. -->
</servlet-mapping>


</web-app>